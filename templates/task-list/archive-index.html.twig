{% extends 'base.html.twig' %}

{% block title %}{{ 'list.your_lists'|trans }}{% endblock %}

{% block body %}
    {% if task_lists %}
        {% for task_list in task_lists %}
            {% include 'parts/private/list/list-block.html.twig' with {'task_list': task_list} %}
        {% endfor %}
    {% else %}
        {% include 'common/no-result.html.twig' only %}
    {% endif %}
{% endblock %}